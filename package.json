{
  "name": "zmbl-ui",
  "version": "1.0.0",
  "author": "zzh",
  "private": false,
  "main": "lib/index/index.js",
  "homepage": "https://sangtian152.gitee.io/zm-mobile-ui",
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitee.com/sangtian152/zm-mobile-ui.git"
  },
  "keywords": [
    "zm-mobile-ui",
    "orgchart",
    "vue",
    "mobile-ui",
    "components"
  ],
  "scripts": {
    "dev": "npm run bootstrap && npm run build:entry && npm run serve",
    "bootstrap": "yarn || npm i",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:entry": "node build/bin/build-entry",
    "build:demo": "node build/bin/build-demo",
    "lint": "vue-cli-service lint",
    "build-lib": "vue-cli-service build --target lib --name zm-tree-org --dest lib src/index.js"
  },
  "dependencies": {
    "array-find-index": "^1.0.2",
    "core-js": "^3.6.5",
    "raf.js": "^0.0.4",
    "vue": "^2.6.11",
    "vue-lazyload": "^1.3.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/component-compiler-utils": "^3.2.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.3",
    "eslint": "^7.32.0",
    "eslint-config-elemefe": "^0.3.0",
    "eslint-plugin-vue": "^6.2.2",
    "json-templater": "^1.2.0",
    "markdown-it": "^12.0.2",
    "markdown-it-anchor": "^6.0.0",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-container": "^3.0.0",
    "markdown-loader": "^6.0.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^10.0.4",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.2.4",
    "transliteration": "^2.1.11",
    "uppercamelcase": "^3.0.0",
    "vue-router": "^3.4.8",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": [
        1,
        {
          "vars": "all",
          "args": "after-used"
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
