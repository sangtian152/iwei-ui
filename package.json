{
  "name": "zmbl-ui",
  "version": "1.0.0",
  "author": "zzh",
  "private": false,
  "main": "lib/zmbl-ui.common.js",
  "homepage": "https://sangtian152.gitee.io/zm-mobile-ui",
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitee.com/sangtian152/zm-mobile-ui.git"
  },
  "keywords": [
    "zmbl-ui",
    "vue",
    "mobile-ui",
    "components"
  ],
  "scripts": {
    "dev": "npm run bootstrap && npm run build:entry && npm run serve",
    "bootstrap": "yarn || npm i",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:entry": "node build/bin/build-entry",
    "build:demo": "node build/bin/build-demo",
    "lint": "vue-cli-service lint",
    "build:utils": "cross-env BABEL_ENV=utils babel src --out-dir lib --ignore src/index.js",
    "build:component": "vue-cli-service build --mode component",
    "build:lib": "vue-cli-service build --mode common && vue-cli-service build --mode component --no-clean",
    "dist": "webpack --config test/webpack.conf.js && webpack --config test/webpack.common.js && webpack --config test/webpack.component.js && npm run build:utils"
  },
  "dependencies": {
    "array-find-index": "^1.0.2",
    "core-js": "^3.6.4",
    "raf.js": "^0.0.4",
    "vue-lazyload": "^1.3.3"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/runtime-corejs3": "^7.15.4",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/component-compiler-utils": "^3.2.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-elemefe": "^0.3.0",
    "eslint-plugin-vue": "^6.2.2",
    "imd-loader": "^1.0.0",
    "json-templater": "^1.2.0",
    "markdown-it": "^12.2.0",
    "markdown-it-anchor": "^8.4.1",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-container": "^3.0.0",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^10.0.4",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^4.2.3",
    "transliteration": "^2.2.0",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^4.1.1",
    "vue": "^2.6.12",
    "vue-router": "^3.4.8",
    "vue-template-compiler": "^2.6.12",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": [
        1,
        {
          "vars": "all",
          "args": "after-used"
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
