{
  "presets": [[
    "@babel/preset-env", 
    {
      "loose": true,
      "modules": false,
      "useBuiltIns": "usage",
      "corejs": 3,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }
  ]],
  /* "plugins": [
    [
      "import",
      {
        "libraryName": "zmbl-ui",
        "style": true
      }
    ]
  ], */
  "env": {
    "development":{
      "presets": ["@vue/app"]
    },
    "utils": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "loose": true,
            "modules": "commonjs",
            "targets": {
              "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
            }
          }
        ]
      ],
      "plugins": [
        ["module-resolver", {
          "root": ["zmbl-ui"],
          "alias": {
            "zmbl-ui/src": "zmbl-ui/lib"
          }
        }]
      ]
    },
    "test": {
      "plugins": ["istanbul"]
    }
  }
}
